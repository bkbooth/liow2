  "license": "ISC",
  "dependencies": {
    "bcrypt": "^0.8.3",
    "body-parser": "^1.13.1",
    "cookie-parser": "^1.3.5",
    "cors": "^2.7.1",
    "debug": "^2.2.0",
    "express": "^4.13.0",
    "jsonwebtoken": "^5.0.2",
    "lodash": "^3.9.3",
    "mongoose": "^4.0.4",
    "morgan": "^1.6.0",
    "passport": "^0.2.2",
    "passport-facebook": "^2.0.0",
    "passport-http-bearer": "^1.0.1",
    "passport-local": "^1.0.0"
  },
  "devDependencies": {
    "blanket": "^1.1.7",
    "chai": "^3.0.0",
    "mocha": "^2.2.5",
    "mocha-lcov-reporter": "0.0.2",
    "supertest": "^1.0.1"
  },
  "config": {
    "blanket": {
      "pattern": [
